    "squizlabs/php_codesniffer": "2.*"
  },
  "keywords": ["database", "orm", "mysql", "sqlite"],
  "homepage": "https://reneschmidt.de/wiki/index.php/page/view/SimpleOrm,Start",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Rene Schmidt",
      "email": "rene+_pkg@reneschmidt.de"
    }
  ],
  "autoload": {
    "psr-4": {
